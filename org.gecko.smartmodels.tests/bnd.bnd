-include: ${workspace}/cnf/include/jdt.bnd

# sets the usually required buildpath using the bnd library, you can extend it with the normal -buildpath to your liking
-library: enable-emf

-buildpath: \
	osgi.annotation;version='7.0',\
	osgi.core;version='7.0',\
	osgi.cmpn;version='7.0',\
	org.gecko.smartmodels.apis,\
	org.gecko.smartmodels.building.model;version=latest,\
	org.gecko.smartmodels.geojson.model;version=latest,\
	org.gecko.emf.json,\
	org.eclipse.emfcloud.emfjson-jackson

Bundle-Version: 1.0.0.SNAPSHOT
Private-Package: org.gecko.smartmodels.tests

-includeresource.resources: \
	data=data/
